		{
			"name": "IP",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type is present\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Length is present\", function () {\r",
							"    pm.response.to.have.header(\"Content-Length\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Length is 32\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Length\")).to.eql(\"32\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type is app/json\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.eql(\"application/json\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Compare IP to foreign API - ipify.org\" ,\r",
							"  async function () {\r",
							"    let fIPjson = await fetch(\"https://api.ipify.org?format=json\");\r",
							"    let fIP = await fIPjson.json(); \r",
							"    var origIP = pm.response.json();\r",
							"    pm.expect(origIP.origin).to.eql(fIP.ip);\r",
							"  }\r",
							");\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
